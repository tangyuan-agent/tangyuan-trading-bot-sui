{
  "name": "tangyuan-trading-bot-sui",
  "version": "0.1.0",
  "description": "Intelligent DeFi trading bot for Sui blockchain",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "setup": "tsx scripts/setup-wallet.ts"
  },
  "keywords": [
    "sui",
    "trading-bot",
    "defi",
    "cryptocurrency",
    "cetus",
    "turbos"
  ],
  "author": "Tangyuan AI",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.18.0",
    "better-sqlite3": "^9.4.0",
    "grammy": "^1.19.2",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
